<div class="center-content">
  <div class="floating-text find-your-space">
    <h1>Find your space.</h1>
      <div class="btn-group select-width">
        <select data-placeholder="Select Your Neighborhood" class="parkfilter chzn-select form-control" id="neighborhoods">
          <option value="">All Neighborhoods</option>
          {{#each neighborhoods}}
            <option value="{{ id }}">{{ name }}</option>
          {{/each}}
        </select>
      </div>

      <div class="btn-group select-width">
        <select data-placeholder="Select Your Activity" class="parkfilter chzn-select form-control" id="facility__activity">
          <option value="">All Activities</option>
          {{#each activities}}
            <option value="{{ id }}">{{ name }}</option>
          {{/each}}
        </select>
      </div>
    <div class="gobutton" id="filter-select">GO</div>
  </div>  
</div>

<div id="carousel-example-generic" data-ride="carousel" class="carousel slide">

  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner" id="carousel-images-container">
    <div class="item active">
      <div class="carousel-image" style="background-image: url('../../../static/img/4-JamaicaPondJP.JPG');" alt="..."></div>
      <div class="carousel-caption"></div>
    </div>
    <div class="item">
      <div class="carousel-image" style="background-image: url('../../../static/img/Nkraft (Charles River Bike Path Sunset 1).jpg');" alt="..."></div>
      <div class="carousel-caption"></div>
    </div>
    <div class="item">
      <div class="carousel-image" style="background-image: url('../../../static/img/13-OMalleyWaterfrontPark-Chelsea.JPG');" alt="..."></div>
      <div class="carousel-caption"></div>
    </div>
  </div>
</div>  

<div class="featured-parks">
  <div id="featured" class="owl-carousel orbs">
    <div class="item"><img src="../../../static/img/owl1.jpg" alt="Owl Image"></div>
    <div class="item"><img src="../../../static/img/owl2.jpg" alt="Owl Image"></div>
    <div class="item"><img src="../../../static/img/owl3.jpg" alt="Owl Image"></div>
    <div class="item"><img src="../../../static/img/owl4.jpg" alt="Owl Image"></div>
    <div class="item"><img src="../../../static/img/owl5.jpg" alt="Owl Image"></div>
    <div class="item"><img src="../../../static/img/owl6.jpg" alt="Owl Image"></div>
    <div class="item"><img src="../../../static/img/owl7.jpg" alt="Owl Image"></div>
  </div>
</div> 
